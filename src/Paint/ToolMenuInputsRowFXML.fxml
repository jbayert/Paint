<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox fx:id="TextSettingsObject" spacing="10.0">
            <children>
                <StackPane>
                    <children>
                        <Label text="Text Size:" />
                    </children>
                </StackPane>
                <Spinner fx:id="TextSizeObject" prefWidth="100.0" />
                <Separator orientation="VERTICAL" />
                <StackPane layoutX="10.0" layoutY="10.0">
                    <children>
                        <Label text="Text" />
                    </children>
                </StackPane>
                <TextField fx:id="TextObject" />
            </children>
        </HBox>
        <HBox fx:id="PolygonSettingsObject" spacing="10.0">
            <children>
                <StackPane>
                    <children>
                        <Label text="Number of Sides:" />
                    </children>
                </StackPane>
                <Spinner fx:id="numberOfSidesObject" prefWidth="100.0" />
            </children>
        </HBox>

        <HBox fx:id="RotateObject" layoutX="325.0" layoutY="13.0" spacing="10.0">
            <children>
                <StackPane>
                    <children>
                        <Label text="Rotate Degrees:" />
                    </children>
                </StackPane>
                <Spinner fx:id="rotateDegrees" prefWidth="100.0" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="3.0" left="10.0" right="10.0" top="3.0" />
    </padding>
</HBox>    
